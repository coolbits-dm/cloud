{
  "version": "0.1",
  "updated": "2025-08-24",
  "scoring_models": {
    "ICE": {
      "fields": [
        "impact",
        "confidence",
        "ease"
      ],
      "scale_hint": "1..10 (higher is better)",
      "formula": "impact * confidence * ease"
    },
    "RICE": {
      "fields": [
        "reach",
        "impact",
        "confidence",
        "effort"
      ],
      "scale_hint": "reach in users/period; impact 0.25/0.5/1/2/3; confidence 0..1; effort in person-weeks (lower better)",
      "formula": "(reach * impact * confidence) / max(0.25, effort)"
    }
  },
  "defaults": {
    "window_days": 14,
    "owner": "growth",
    "measurement": {
      "stats": "directional",
      "min_runtime_days": 7,
      "segmentation": [
        "device",
        "geo",
        "new_vs_returning"
      ]
    },
    "cbExpr_defaults": {
      "days": 30,
      "tz": "Europe/Bucharest"
    }
  },
