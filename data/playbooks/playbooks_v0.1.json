      "code": "PB:ADS:BRAND_DEFENSE_SPLIT",
      "area": "ADS",
      "title": {
        "ro": "Separă brand vs non‑brand + protecție brand",
        "en": "Split brand vs non‑brand + brand defense"
      },
      "preconditions": "exists_metric('METRIC:CLICKS',30) AND avg('METRIC:IMPR_SHARE',30) < 0.9",
      "hypothesis": {
        "ro": "Separarea va crește controlul pe brand și ROAS per segment.",
        "en": "Splitting will improve control on brand and ROAS per segment."
      },
      "steps": [
        {
          "ro": "Creează campanii separate: Brand, Non‑brand, Competitor, PMax (dacă e cazul).",
          "en": "Create separate campaigns: Brand, Non-brand, Competitor, PMax as applicable."
        },
        {
          "ro": "Aplică negative lists în Non‑brand pentru termeni brand.",
          "en": "Apply negative lists in Non-brand for brand terms."
        },
        {
          "ro": "Crește tROAS (sau bid) pe Brand pentru acoperire ~100%.",
          "en": "Raise tROAS (or bids) on Brand to target ~100% coverage."
        }
      ],
      "measurement_plan": {
        "primary_metrics": [
          "METRIC:ROAS",
          "METRIC:CPA",
          "METRIC:IMPR_SHARE"
        ],
        "secondary_metrics": [
          "METRIC:SEARCH_LOST_IS_BUDGET",
          "METRIC:SEARCH_LOST_IS_RANK"
        ],
        "window_days": 14
      },
      "scoring": {
        "ICE": {
          "impact": 8,
          "confidence": 7.0,
          "ease": 7
        },
        "RICE": {
          "reach": 1000,
          "impact": 1,
          "confidence": 0.7,
          "effort": 1.5
        }
      },
      "guardrails": [
        "Do not lower Brand coverage below 95%."
      ]
    },
